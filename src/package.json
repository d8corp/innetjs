{
  "name": "innetjs",
  "version": "2.0.5",
  "description": "CLI for innet boilerplate",
  "homepage": "https://github.com/d8corp/innetjs",
  "author": "Mikhail Lysikov <d8corp@mail.ru>",
  "license": "MIT",
  "private": true,
  "bin": {
    "innetjs": "bin/innet"
  },
  "scripts": {
    "build": "bin/release -rollup",
    "test": "jest",
    "start": "npm run build && cd lib && npm link",
    "stop": "npm unlink innetjs -g",
    "restart": "npm run stop && npm start",
    "patch": "bin/release -patch -rollup -publish",
    "minor": "bin/release -minor -rollup -publish",
    "major": "bin/release -major -rollup -publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d8corp/innetjs.git"
  },
  "keywords": [
    "watch",
    "state",
    "stateful",
    "reaction",
    "reactions",
    "react",
    "reactive",
    "observe",
    "observable",
    "framework",
    "view",
    "cli",
    "front-end"
  ],
  "bugs": {
    "url": "https://github.com/d8corp/innetjs/issues"
  },
